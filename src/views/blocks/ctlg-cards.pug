    .catalogue-card__container.js-animated
      .catalogue-card__image
        a(href="#").cover-box.cover-box_contain
          img(data-src=href alt="image").lazy
      .catalogue-card__content
        .catalogue-card__top
          a(href="#").catalogue-card__title.t-size-24=title
          p=text
          